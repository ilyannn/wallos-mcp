# Wallos MCP Server Configuration
# Copy this file to .env and configure for your Wallos instance

# Wallos instance URL (default: http://localhost:8282)
WALLOS_URL=http://localhost:8282

# Wallos API key (required)
# Get this from your Wallos instance: Settings -> User Management -> API Keys
WALLOS_API_KEY=your_api_key_here

# Optional: Set log level for debugging
# Levels: error, warn, info, debug
LOG_LEVEL=info
